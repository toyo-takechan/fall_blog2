<header class="header" >
  <div class="header-left" >
    <%= link_to image_tag("icons8-kaede.png", alt:"logo", width: "60px", class: "header-left-logo"), root_url %>
    <%= link_to "Fall Blog", root_url, class: "header-left-title"%>
  </div>
  <div class="header-middle" >
    <input type="text" size="50" placeholder="キーワード検索">
  　<input type="submit" value="ダミー">
  </div>
  <div class="header-right" >
    <% if logged_in? %>
    <div class="header-right-cell "> <%= link_to "ログアウト", logout_path, data:{turbo_method: :delete }%> </div>
    <% else %>
    <div class="header-right-cell "> <%= link_to "ログイン", login_path %> </div>
    <% end %>
    <div class="header-right-cell "> <%= link_to "新規登録", signup_path %> </div>

  </div>
</header>
